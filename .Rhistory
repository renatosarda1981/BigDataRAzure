grafico <- function(n,p){
x <- dbinom(0:n, size = n, prob = p)
barplot(x, ylim = c(0, 0.4), names.arg = 0:n,
main = sprintf(paste("Binomial Distribution(n,p)", n, p, sep = ",")))
}
grafico(10, 0.8)
